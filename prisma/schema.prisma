datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Definition {
    id         String  @id() @default(cuid())
    word       String  @unique
    definition String?
    english    String?
    swahili    String?
    type       String? @default("word")
    dialect    String  @default("all")
    example    String?
}
